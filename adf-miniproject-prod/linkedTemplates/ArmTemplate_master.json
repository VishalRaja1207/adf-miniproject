{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-miniproject-prod"
		},
		"LS_ABLB_Misumi_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ABLB_Misumi'"
		},
		"LS_MONG_Inventory_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_MONG_Inventory'"
		},
		"LS_SQL_Inventory_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQL_Inventory'"
		},
		"LS_SQL_Misumi_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQL_Misumi'"
		},
		"LS_MONG_Inventory_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Inventory"
		},
		"LS_SQL_Inventory_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserverminiproject.database.windows.net"
		},
		"LS_SQL_Inventory_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sql-Inventory-dev"
		},
		"LS_SQL_Inventory_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vishal"
		},
		"LS_SQL_Misumi_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserverminiproject.database.windows.net"
		},
		"LS_SQL_Misumi_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sql-misumi-dev"
		},
		"LS_SQL_Misumi_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vishal"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-miniproject-prod_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ABLB_Misumi_connectionString": {
						"value": "[parameters('LS_ABLB_Misumi_connectionString')]"
					},
					"LS_MONG_Inventory_connectionString": {
						"value": "[parameters('LS_MONG_Inventory_connectionString')]"
					},
					"LS_SQL_Inventory_password": {
						"value": "[parameters('LS_SQL_Inventory_password')]"
					},
					"LS_SQL_Misumi_password": {
						"value": "[parameters('LS_SQL_Misumi_password')]"
					},
					"LS_MONG_Inventory_properties_typeProperties_database": {
						"value": "[parameters('LS_MONG_Inventory_properties_typeProperties_database')]"
					},
					"LS_SQL_Inventory_properties_typeProperties_server": {
						"value": "[parameters('LS_SQL_Inventory_properties_typeProperties_server')]"
					},
					"LS_SQL_Inventory_properties_typeProperties_database": {
						"value": "[parameters('LS_SQL_Inventory_properties_typeProperties_database')]"
					},
					"LS_SQL_Inventory_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQL_Inventory_properties_typeProperties_userName')]"
					},
					"LS_SQL_Misumi_properties_typeProperties_server": {
						"value": "[parameters('LS_SQL_Misumi_properties_typeProperties_server')]"
					},
					"LS_SQL_Misumi_properties_typeProperties_database": {
						"value": "[parameters('LS_SQL_Misumi_properties_typeProperties_database')]"
					},
					"LS_SQL_Misumi_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQL_Misumi_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}